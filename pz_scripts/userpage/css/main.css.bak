/*
	Span Tag System
*/
span .tag {
  color: #FFF;
  background-color: transparent;
}
span .tag .spanActive {
  color: #000;
  background-color: #FFFF00;
}
span .tag .spanMatch {
  color: #000;
  background-color: #00FF00;
}
#tagselector {
  border-style: solid;
  border-color: #00aaff;
  border-width: 1px;
}
#tagselector p {
  margin-bottom: 0px;
  margin-top: 0px;
}
#tagselector label {
  display: block;
}
#tagselector label:hover {
  background-color: #888;
}
#tagBlocker {
  z-index: 200;
  background-color: rgba(80, 80, 80, 0.8);
  display: none;
  position: absolute;
  top: 0px;
  left: 0px;
}
div.refresh {
  position: absolute;
  top: 0.5em;
  right: 0.5em;
  background: url('../images/userpage_refresh_rest.png') 0 0;
  width: 48px;
  height: 48px;
}
div.refresh.active {
  background: url('../images/userpage_refresh_full.png') 0 0;
}

div.pageContainer {
	height: 100%
}

.splitterCss {
	width: 6px;
	background: initial;
	background-color: #00aaff;
	height: 100%;
	margin-left: 270px;
	background: -webkit-repeating-linear-gradient(
		45deg,
		#00AAFF,
		#00AAFF 10px,
		#CDEFFF 10px,
		#CDEFFF 20px
	);
	background: -o-repeating-linear-gradient(
		45deg,
		#00AAFF,
		#00AAFF 10px,
		#CDEFFF 10px,
		#CDEFFF 20px
	);
	background: -moz-repeating-linear-gradient(
		45deg,
		#00AAFF,
		#00AAFF 10px,
		#CDEFFF 10px,
		#CDEFFF 20px
	);
	background: repeating-linear-gradient(
		45deg,
		#00AAFF,
		#00AAFF 10px,
		#CDEFFF 10px,
		#CDEFFF 20px
	);	
}

.splitterHoverCss {
	width: 6px;
	background: -webkit-repeating-linear-gradient(
		45deg,
		#999,
		#999 10px,
		#CCC 10px,
		#CCC 20px
	);
	background: -o-repeating-linear-gradient(
		45deg,
		#999,
		#999 10px,
		#CCC 10px,
		#CCC 20px
	);
	background: -moz-repeating-linear-gradient(
		45deg,
		#999,
		#999 10px,
		#CCC 10px,
		#CCC 20px
	);
	background: repeating-linear-gradient(
		45deg,
		#999,
		#999 10px,
		#CCC 10px,
		#CCC 20px
	);	
}

