/* definitions */
.fixleft {
  left: 0px;
}
.fixright {
  right: 0px;
}
.fixtop {
  top: 0px;
}
.fixbottom {
  bottom: 0px;
}
/* body */
:root {
  font-size: 100%;
}
*,
*:before,
*:after {
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  /* Safari/Chrome, other WebKit */
  -moz-box-sizing: border-box;
  /* Firefox, other Gecko */
}
html {
  overflow: hidden;
  -webkit-touch-callout: none;
  -webkit-user-select: none;
  -khtml-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  /* Safari/Chrome, other WebKit */
  -moz-box-sizing: border-box;
  /* Firefox, other Gecko */
}
body {
  font-family: Arial, sans-serif;
  font-size: 1em;
  background-color: #ffffff;
  color: #000000;
}
h1 {
  font-family: 'Segoe UI', Frutiger, 'Frutiger Linotype', 'Dejavu Sans', 'Helvetica Neue', Arial, sans-serif;
  font-weight: 100;
}
h3 {
  font-family: 'Segoe UI', Frutiger, 'Frutiger Linotype', 'Dejavu Sans', 'Helvetica Neue', Arial, sans-serif;
  font-weight: 100;
}
p {
  font-family: 'Segoe UI', Frutiger, 'Frutiger Linotype', 'Dejavu Sans', 'Helvetica Neue', Arial, sans-serif;
  font-weight: normal;
}
blockquote {
  font-family: 'Segoe UI', Frutiger, 'Frutiger Linotype', 'Dejavu Sans', 'Helvetica Neue', Arial, sans-serif;
  font-weight: normal;
}
pre {
  font-family: 'Segoe UI', Frutiger, 'Frutiger Linotype', 'Dejavu Sans', 'Helvetica Neue', Arial, sans-serif;
  font-weight: normal;
}
a {
  text-decoration: none;
}
a:link {
  color: #000000;
}
a:visited {
  color: #000000;
}
a:focus {
  color: #000000;
}
a:hover {
  color: #0000ff;
}
a:active {
  color: #0000ff;
}
.main {
  position: absolute;
  margin: 0px;
  top: 0px;
  bottom: 0px;
  left: 0px;
  right: 0px;
}
.header {
  background: #00aaff url("../images/ui-bg_highlight-soft_75_00aaff_1x100.png") 50% 50% repeat-x;
  color: #ffffff;
  height: 4.5rem;
}
.header .title {
  margin: 0%;
}
.header .email {
  margin: 0%;
}
.header div {
  text-align: center;
}
/* left pane */
.leftpane {
  position: absolute;
  width: 270px;
  min-width: 270px;
  display: inline;
  float: left;
  top: 4.5rem;
  left: 0px;
  bottom: 0px;
}
.sortingmenu {
  width: 50px;
  float: left;
  height: 100%;
  border-style: solid;
  border-color: #00aaff;
  border-width: 1px;
  left: 0px;
  top: 0px;
  right: 0px;
}
.samplelist {
  height: 100%;
  border-style: solid;
  border-color: #00aaff;
  border-width: 1px;
  left: 50px;
  overflow: hidden;
  overflow-y: scroll;
}
.samplelist table {
  width: 100%;
}
.samplelist table img {
  border-style: solid;
  border-color: #00aaff;
  border-width: 1px;
  margin: 5%;
  width: 90%;
  height: auto;
}
/* right pane */
.rightpane {
  position: absolute;
  top: 4.5rem;
  right: 0px;
  bottom: 0px;
  left: 276px;
}
.rightpane .centerPage {
  height: 100%;
  top: 0%;
  width: 100%;
  left: 0%;
}
.rightpane .centerPage .raster {
  height: 100%;
  border-style: solid;
  border-color: #00aaff;
  border-width: 1px;
  bottom: 0px;
}
.rightpane .centerPage .sampleCell {
  border-style: solid;
  border-color: #00aaff;
  border-width: 1px;
}
.selectionList {
  border-style: solid;
  border-color: #00aaff;
  border-width: 1px;
  width: 90%;
  height: 50%;
  overflow-y: scroll;
}
/* elements */
div.icon.trash {
  position: absolute;
  margin: 5px;
  top: 0px;
  right: 0px;
  background: url('../images/userpage_delete_rest.png') 0 0;
}
div.icon.trash:hover {
  background: url('../images/userpage_delete_hover.png') 0 0;
}
div.icon.trash:active {
  background: url('../images/userpage_delete_pressed.png') 0 0;
}
div.icon.alpha {
  position: absolute;
  margin: 5px;
  top: 0px;
  right: 32px;
  background: url('../images/userpage_alpha_rest.png') 0 0;
}
div.icon.alpha:hover {
  background: url('../images/userpage_alpha_hover.png') 0 0;
}
div.icon.alpha:active {
  background: url('../images/userpage_alpha_pressed.png') 0 0;
}
div.icon.alpha.active {
  background: url('../images/userpage_alpha_pressed.png') 0 0;
}
